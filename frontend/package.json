  },
  "devDependencies": {
    "@types/react-modal": "^3.13.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint-plugin-react": "^7.31.10"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/react-app-env.d.ts",
      "!src/reportWebVitals.ts",
